<!DOCTYPE html>
<html>
    <head>


        <link href="https://ubagoduona.lt/styleelements/messengerShare.png" rel="apple-touch-icon">
        <!-- Global site tag (gtag.js) - Google Analytics -->
        <script async src="https://www.googletagmanager.com/gtag/js?id=UA-123608222-1"></script>
        <script>
            window.dataLayer = window.dataLayer || [];
            function gtag(){dataLayer.push(arguments);}
            gtag('js', new Date());

            gtag('config', 'UA-123608222-1');
        </script>

        {% block head %}


        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="icon" href="https://ubagoduona.lt/styleelements/favicon.png">
            <meta property="fb:app_id" content="2064495207212947" />
            <meta property="fb:admins" content="saulius.usonis" />
            <meta property="og:url" content="{{ app.request.uri }}" />


            {% if post.title is defined %}
                <meta property="og:title" content="{{ post.title }}" />
            {% else %}
                <meta property="og:title" content="Ubago Duona" />

            {%  endif %}


            {% if post.content is defined %}
                <meta property="og:description" content="{{ post.content|striptags }}" />
            {% else %}
                <meta property="og:description" content="Ubago Duona kultÅ«ros/absurdo trupiniai" />

            {%  endif %}

            {% if post.coverImage is defined %}
                <meta property="og:image" content="https://ubagoduona.lt{{ post.coverImage }}" />
            {% else %}
                <meta property="og:image" content="https://ubagoduona.lt/styleelements/messengerShare.png" />

            {%  endif %}


            <meta property="og:type" content="website" />

        <title>

            {% block title %}
                Ubago Duona
            {% endblock %}

        </title>

            {% block stylesheets %}
                <style>
                    body{
                        margin: 0 !important;
                        padding: 0 !important;
                    }
                </style>
                <link rel="stylesheet" type="text/css" href="{{ asset("build/css/parallax.css") }}">
                <link rel="stylesheet" type="text/css" href="{{ asset("build/css/app.css") }}">
                <link rel="stylesheet" type="text/css" href="{{ asset("build/css/navbar.css") }}">
                <link rel="stylesheet" type="text/css" href="{{ asset("build/css/archive.css") }}">
                <link rel="stylesheet" type="text/css" href="{{ asset("build/css/sidebar.css") }}">
                <link rel="stylesheet" type="text/css" href="{{ asset("build/css/showPost.css") }}">
                <link rel="stylesheet" type="text/css" href="{{ asset("build/css/authorShow.css") }}">
                <link rel="stylesheet" type="text/css" href="{{ asset("build/css/authoredit.css") }}">
                <link rel="stylesheet" type="text/css" href="{{ asset("build/css/authorShowAll.css") }}">
                <link rel="stylesheet" type="text/css" href="{{ asset("build/css/postEdit.css") }}">
                <link rel="stylesheet" type="text/css" href="{{ asset("build/css/autoriausKampelis.css") }}">
                <link rel="stylesheet" type="text/css" href="{{ asset("build/css/responsiveIphone5.css") }}">
                <link rel="stylesheet" type="text/css" href="{{ asset("build/css/footer.css") }}">

            {% endblock %}

        {% endblock %}
    </head>

    <body>
    <div id="fb-root"></div>
    <script>(function(d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) return;
            js = d.createElement(s); js.id = id;
            js.src = 'https://connect.facebook.net/lt_LT/sdk.js#xfbml=1&version=v3.1&appId=2064495207212947';
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));</script>
        <div style="height: 100%">
        {% block parallax %}
        {% endblock %}
        <div class="after-parallax">
            {% block navbar %}
                {% include 'navbar.html.twig' %}
            {% endblock %}

            {% block sidebar %}
                {% include 'sidebar.html.twig' %}
            {% endblock %}

            {% block body %}
                <div class="after-navbar"></div>
            {% endblock %}




        </div>
        </div>
        {% block footer %}
            {% include 'footer.html.twig' %}
        {% endblock %}
        {% block javascripts %}
            <script src="{{ asset('build/js/app.js') }}"></script>
            <script src="{{ asset('build/js/dropdownmenu.js') }}" type='text/javascript'></script>
        {% endblock %}
    </body>
</html>
