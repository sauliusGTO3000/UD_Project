<!DOCTYPE html>
<html>
    <head>
        <!-- Global site tag (gtag.js) - Google Analytics -->
        <script async src="https://www.googletagmanager.com/gtag/js?id=UA-123608222-1"></script>
        <script>
            window.dataLayer = window.dataLayer || [];
            function gtag(){dataLayer.push(arguments);}
            gtag('js', new Date());

            gtag('config', 'UA-123608222-1');
        </script>

        {% block head %}

        <meta property="og:image" content="/styleelements/messengerShare.png">
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="icon" href="/styleelements/favicon.png">

        <title>

            {% block title %}
                Ubago Duona
            {% endblock %}

        </title>

            {% block stylesheets %}
                <style>
                    body{
                        margin: 0 !important;
                        padding: 0 !important;
                    }
                </style>
                <link rel="stylesheet" type="text/css" href="{{ asset("build/css/parallax.css") }}">
                <link rel="stylesheet" type="text/css" href="{{ asset("build/css/app.css") }}">
                <link rel="stylesheet" type="text/css" href="{{ asset("build/css/navbar.css") }}">
                <link rel="stylesheet" type="text/css" href="{{ asset("build/css/archive.css") }}">
                <link rel="stylesheet" type="text/css" href="{{ asset("build/css/sidebar.css") }}">
                <link rel="stylesheet" type="text/css" href="{{ asset("build/css/showPost.css") }}">
                <link rel="stylesheet" type="text/css" href="{{ asset("build/css/authorShow.css") }}">
                <link rel="stylesheet" type="text/css" href="{{ asset("build/css/authoredit.css") }}">
                <link rel="stylesheet" type="text/css" href="{{ asset("build/css/authorShowAll.css") }}">
                <link rel="stylesheet" type="text/css" href="{{ asset("build/css/postEdit.css") }}">
                <link rel="stylesheet" type="text/css" href="{{ asset("build/css/autoriausKampelis.css") }}">
                <link rel="stylesheet" type="text/css" href="{{ asset("build/css/responsiveIphone5.css") }}">
                <link rel="stylesheet" type="text/css" href="{{ asset("build/css/footer.css") }}">

            {% endblock %}

        {% endblock %}
    </head>

    <body>

        {% block parallax %}
        {% endblock %}
        <div class="after-parallax">
            {% block navbar %}
                {% include 'navbar.html.twig' %}
            {% endblock %}

            {% block sidebar %}
                {% include 'sidebar.html.twig' %}
            {% endblock %}

            {% block body %}
                <div class="after-navbar"></div>
            {% endblock %}

            {% block footer %}

            {% endblock %}


        </div>


        {% block javascripts %}
            <script src="{{ asset('build/js/app.js') }}"></script>
        {% endblock %}
    </body>
</html>
